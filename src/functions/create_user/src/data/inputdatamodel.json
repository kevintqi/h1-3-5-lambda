{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "title": "User",
  "description": "Data schema for User",
  "type": "object",
  "properties": {
    "userId": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "phoneNumber": {
      "type": "string"
    },
    "picture": {
      "type": "string"
    },
    "name": {
      "type": "object",
      "properties": {
        "first": {
          "type": "string"
        },
        "last": {
          "type": "string"
        },
        "alias": {
          "type": "string"
        } 
      },
      "required": ["first", "last"]
    }
  },
  "required": ["name", "email", "phoneNumber"]
}
